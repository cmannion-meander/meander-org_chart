# Public (safe for browser bundle)
NEXT_PUBLIC_SUPABASE_URL=https://your-project-ref.supabase.co
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_DEFAULT_KEY=sb_publishable_your_key_here

# Server-only secrets (never expose in NEXT_PUBLIC_ variables)
SUPABASE_SECRET_DEFAULT_KEY=sb_secret_your_key_here
SUPABASE_DB_PASSWORD=your-db-password

# Optional direct SQL connection string (for psql/migrations)
DATABASE_URL=postgresql://postgres:your-db-password@your-pooler-host:6543/postgres?sslmode=require

# Optional legacy aliases (must match canonical values if both are set)
# NEXT_PUBLIC_SUPABASE_ANON_KEY=sb_publishable_your_key_here
# SUPABASE_SERVICE_ROLE_KEY=sb_secret_your_key_here

# Never set this variable. It exposes server secrets to browser bundles.
# NEXT_PUBLIC_SUPABASE_SECRET_DEFAULT_KEY=
